<Window x:Class="WpfApp2.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp2"
        xmlns:sprie="clr-namespace:WpfApp2.Pages"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="200"/>
            <ColumnDefinition Width="225"/>
            <ColumnDefinition Width="80"/>
            <ColumnDefinition Width="*"/>
            <ColumnDefinition Width="80"/>
        </Grid.ColumnDefinitions>
       
        <sprie:PaintInstrumetion Grid.Column="4"></sprie:PaintInstrumetion>
        <sprie:PanelPaint Grid.Column="3"></sprie:PanelPaint>
        <sprie:Compilar x:Name="compilar" Grid.Column="1"></sprie:Compilar>
        <sprie:Functional></sprie:Functional>
        
        
    </Grid>
</Window>
